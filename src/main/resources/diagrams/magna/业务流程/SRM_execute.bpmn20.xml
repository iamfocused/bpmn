    <scriptTask id="scripttask2" name="更新采购计划" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>
      
    		import groovy.json.JsonSlurper
    		import groovy.json.JsonOutput
    		
    		def jsonSlurper = new JsonSlurper()
    		def processInstId = jsonSlurper.parseText(execution.getVariable('chooseplan')).code
    		
    		def planExecution = runtimeService.createExecutionQuery().processInstanceId(processInstId).singleResult();
    		if(planExecution == null) {
    			throw new org.activiti.engine.ActivitiIllegalArgumentException('采购计划已被删除，流程ID是：' + processInstId)
    		}
    		def tbhddetail_item = jsonSlurper.parseText(planExecution.getVariable('tbhddetail_item'))
    		def tbhddetail_item_std = jsonSlurper.parseText(planExecution.getVariable('tbhddetail_item_std'))
    		
    		//本地var
    		def std = jsonSlurper.parseText(execution.getVariable('tbhddetail_item_std'))
    		
    		tbhddetail_item.each{k,v -&gt;
    			if(k == 'rows') {
    				v.each{item -&gt;
			            def find = std.find{it.matinfo == item[0]}
			            if(find){
			                item[2] = item[2].toBigDecimal().subtract(find.accnumber.toBigDecimal())
			            }
			        }
    			}
    		}
    		
    		tbhddetail_item_std.each{item -&gt;
    			def find = std.find{it.matinfo == item.matinfo}
			    if(find) {
			        item.remainqty = item.remainqty.toBigDecimal().subtract(find.accnumber.toBigDecimal())
			    }
    		}
    		
    		planExecution.setVariable('tbhddetail_item',JsonOutput.toJson(tbhddetail_item))
    		planExecution.setVariable('tbhddetail_item_std',JsonOutput.toJson(tbhddetail_item_std))
    	
    	</script>
    </scriptTask>
