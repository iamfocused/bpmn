      <![CDATA[
    		import groovy.json.JsonSlurper
    		
    		def jsonSlurper = new JsonSlurper()
    		def processInstId = jsonSlurper.parseText(execution.getVariable('chooseplan')).code
    		
    		def planExecution = runtimeService.createExecutionQuery().processInstanceId(processInstId).singleResult();
    		if(planExecution == null) {
    			throw new org.activiti.engine.ActivitiIllegalArgumentException('采购计划已被删除，流程ID是：' + processInstId)
    		}
    		def info = planExecution.getVariable('tbhddetail_item')
    		if(info == null) {
    			throw new org.activiti.engine.ActivitiIllegalArgumentException('采购计划信息数据异常，流程ID是：' + processInstId)
    		}
    		execution.setVariable('tbhddetail_item', info)
    		
    		execution.setVariable('VND_ID_',planExecution.getVariable('VND_ID_'))
    		execution.setVariable('PLAN_FROM_',planExecution.getVariable('PLAN_FROM_'))
    		execution.setVariable('PLAN_TO_',planExecution.getVariable('PLAN_TO_'))
    	]]>
    	</script>
